{
  "npmScope": "social-network",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "sn-application": {
      "tags": []
    },
    "social-network-frontend": {
      "tags": []
    },
    "social-network-frontend-e2e": {
      "tags": [],
      "implicitDependencies": [
        "social-network-frontend"
      ]
    }
  },
  "plugins": [
    "@jnxplus/nx-boot-gradle"
  ]
}
